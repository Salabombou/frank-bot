{
  "name": "frank-bot",
  "version": "1.0.0",
  "description": "Discord bot for anonymous messages in a Discord server",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint",
    "build": "tsc -P tsconfig.prod.json",
    "dev": "tsc -P tsconfig.dev.json -w",
    "start": "NODE_PATH=./build node build/index.js",
    "test": "tsc -P tsconfig.test.json && jest"
  },
  "keywords": [
    "discord",
    "typescript"
  ],
  "author": "Salabombou",
  "license": "MIT",
  "dependencies": {
    "async-mutex": "^0.5.0",
    "discord.js": "^14.16.2",
    "dotenv": "^16.4.5",
    "typescript": "^5.6.2",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@eslint/js": "^9.11.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.13",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript-eslint": "^8.7.0"
  }
}
